<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:self="clr-namespace:PatchReviewer">
    <Style TargetType="{x:Type self:ReviewWindow}">
        <Setter Property="OffsetPatchBrush" Value="#E0E0E0" />
        <Setter Property="GoodPatchBrush" Value="#87CEFA" />
        <Setter Property="WarningPatchBrush" Value="#FFA500" />
        <Setter Property="BadPatchBrush" Value="#FF4500" />
        <Setter Property="FailPatchBrush" Value="#FF3C3C" />
    </Style>
    <Style TargetType="{x:Type self:SideBySide}">
        <Setter Property="LeftUnmatchedBrush" Value="#FF8C8C" />
        <Setter Property="RightUnmatchedBrush" Value="#80FF80" />
        <Setter Property="MatchedBrush" Value="#ADD8E6" />
        <Setter Property="MatchedHighlightBrush" Value="#BFEFFF" />
        <Setter Property="OffsetBrush" Value="#D3D3D3" />
        <Setter Property="PatchRangeBrush" Value="#000000" />
        <Setter Property="EditRangeBrush" Value="#808080" />
    </Style>
</ResourceDictionary>